{
  "manifest_version": 3,
  "name": "Image Classifier Extension",
  "version": "1.0",
  "description": "Classifies images using MobileNet and TensorFlow.js.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/content.js"],
      "run_at": "document_idle"
    }
  ]
}
